package petz.com.br.handler;

public class ErroFormularioDto {
	
	private String campo;
	private String mesagem;
	
	public ErroFormularioDto(String campo, String mensagem) {
		this.campo = campo;
		this.mesagem = mensagem;
	}

	public String getCampo() {
		return campo;
	}

	public String getMesagem() {
		return mesagem;
	}
}