package br.com.ztech.eum;

import java.math.BigDecimal;

public enum ConfiguracaoPorcentagemEnum {
	
	//botar isso aqui para vim do banco de dados para facilitar a manutenção

	DONUS_PORCETAGEM_BONUS(1),
	DONUS_PORCETAGEM_CUSTO_RETIRADA_DINHEIRO(2);
	
	private Integer valor;

	ConfiguracaoPorcentagemEnum(Integer valor) {
		this.valor = valor;
	}

	public Integer getValor() {
		return valor;
	}
}