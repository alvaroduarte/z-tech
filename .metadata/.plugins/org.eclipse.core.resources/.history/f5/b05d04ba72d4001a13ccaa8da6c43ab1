package br.com.ztech.domain;

import java.math.BigDecimal;

public enum ValoDonusBonusEnum {

	DONUS_BONUS(new BigDecimal(0.05)),
	DONUS_CUSTO_RETIRADA_DINHEIRO(new BigDecimal(1));
	
	private BigDecimal valor;

	ValoDonusBonusEnum(BigDecimal valor) {
		this.valor = valor;
	}

	public BigDecimal getValor() {
		return valor;
	}
}