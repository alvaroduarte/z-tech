package br.com.ztech.token.eum;

public enum TipoTransacaoEum {

	DEPOSITO_DINHEIRO(1),
	RETIRAR_DINHEIRO(2),
	TRANSFERENCIA_DINHEIRO(3),
	ENTRADA_DINHEIRO_POR_TRANSFERENCIA(4);

	private Integer codigo;

	TipoTransacaoEum(Integer codigo) {
		this.codigo = codigo;
	}

	public Integer getCodigo() {
		return codigo;
	}
}